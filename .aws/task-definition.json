          {
            "containerName": "api",
            "condition": "HEALTHY"
          }
        ],
        "readonlyRootFilesystem": false
      },
      {
        "name": "api",
        "image": "<image>",
        "cpu": 0,
        "memoryReservation": 512,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "entryPoint": [],
        "command": [],
        "environment": [],
        "mountPoints": [],
        "volumesFrom": [],
